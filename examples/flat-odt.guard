        || (t has word)
        || (t has symbol)
        || (t has sequence)
        || (t has span)
        || (t has pagenumber)
        || (t has link);
raw_paragraphcount = root[0].metadata_paragraphcount;
claimed_paragraphcount = int!(raw_paragraphcount[1:len!(raw_paragraphcount)-1]);
