            "text:name=\"Figure\""
            "text:formula=\"ooow:Figure+1\""
            "style:num-format=\"1\""
            ">"
            "1"
        "</text:sequence>"
    |
        "<text:span" #span
            ("text:style-name=" /'\"' "T" ['1'-'9'] '\"'/)
            ">"
            text+
        "</text:span>"
    |
        "<text:page-number" #pagenumber
            "text:select-page=\"current\""
            ">"
            "1"
        "</text:page-number>"
    |
        "<text:a" #link
            "xlink:type=\"simple\""
            "xlink:href=\"mailto:zthannhauser@gmail.com\""
            "text:style-name=\"Internet_20_link\""
            "text:visited-style-name=\"Visited_20_Internet_20_Link\""
            ">"
            text
        "</text:a>"
    ) ;

header:
    "<text:h"
        "text:style-name=" /'\"' ( ("Heading_20" ("_1" | "_2"))
                                 | "P1") '\"'/
        "text:outline-level=" /'\"' ['1'-'2'] '\"'/ ("/>" | ">"
         
         (text #text)+
         
    "</text:h>")
    ;


paragraph:
    "<text:p"
        "text:style-name=" /'\"' ( "Table_20_Contents"
                                 | "Text_20_body"
                                 | "Standard"
                                 | "Figure"
                                 | "Preformatted_20_Text"
                                 | "P" ['1'-'9'] ) '\"'/ ("/>" | ">"
         
        ( text #text | image #image )+
         
    "</text:p>")
    ;

image:
    "<draw:frame"
        "draw:style-name=" /'\"' "fr" ("1" | "2") '\"'/
        "draw:name=" /'\"' ("Frame1" | "my favorate image") '\"'/
        "text:anchor-type=\"char\""
        "svg:x=\"0.7862in\""?
        "svg:y=\"0.1756in\""?
        "svg:width=" /'\"' ['0'-'9'] '.' ['0'-'9']{,4} "in" '\"'/
        "style:rel-width=\"36%\""?
        "svg:height=\"2.3874in\""?
        "style:rel-height=\"scale\""?
        "draw:z-index=" /'\"' ("0" | "1") '\"'/
        ">"
        
        (
            "<draw:image" #data
                "loext:mime-type=\"image/x-MS-bmp\""
                ">"
                
                "<office:binary-data>"
                    /['a'-'z','A'-'Z','/','0'-'9','=','+']{36}/+
                "</office:binary-data>"
            "</draw:image>"
        |
            "<draw:text-box" #caption
                "fo:min-height=\"1.111in\""
                ">"
                paragraph
            "</draw:text-box>"
        )
        
    "</draw:frame>";

list:
    "<text:list"
       ("xml:id=" /'\"' "list" ['0'-'9']{10}'\"'/)?
       ("text:style-name=" /'\"' 'L' ['1'-'2'] '\"'/)?
        ">"
        
       ("<text:list-item>" content+ "</text:list-item>")+
   "</text:list>"
    ;

table:
    "<table:table"
        "table:name=\"Table1\""
        "table:style-name=\"Table1\"" ">"

        // one or more column declarations:
        (
            "<table:table-column"
                "table:style-name=" /'\"' "Table" "1" "." ['A'-'Z']'\"'/
                ("table:number-columns-repeated=\"2\"")?
                "/>"
        )+
        
        // one or more row descriptions:
        (
            "<table:table-row"
                "table:style-name=" /'\"' "Table" ("Line94678479607056" | ['1'-'9'] '.' ['1'-'9']) '\"'/
                ">"
                
                // one or more cell descriptions:
                (
                    "<table:table-cell"
                        "table:style-name=" /'\"' "Table" "1" "." ['A'-'Z']['0'-'9']{,2} '\"'/
                        ("table:number-columns-spanned=" /'\"' ['1'-'9'] '\"'/)?
                        ("table:number-rows-spanned=" /'\"' ['1'-'9'] '\"'/)?
                        "office:value-type=\"string\""
                        ">"
                        
                        paragraph
                    
                    "</table:table-cell>"
                    
                    |
                    
                    "<table:covered-table-cell/>"
                )+
                
            "</table:table-row>"
        )+
        
    "</table:table>"
    ;


// number of tables in metadata must match the actual number of tables:
