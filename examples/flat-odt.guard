actual_paragraphcount = 0
    + len!(filter!($paragraph p -> bool: (p has text) && text_has_content(p.text), paragraph))
    + len!(filter!($header h -> bool: (h has text) && text_has_content(h.text), header));
//%print: actual_paragraphcount;
%error: claimed_paragraphcount == actual_paragraphcount;

