boolean: /bool: "true" | "false"/ #value;

integer: /int: ['0'-'9']+/ #value;

decimal: /float: ['0'-'9']+ '.' ['0'-'9']+/ #value;

string: /['a'-'z', 'A'-'Z']+/ #value;

