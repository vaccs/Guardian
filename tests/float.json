
[{
	"files": {
		"./maia": "bin/test-build/yes-verbose/no-dotout/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"decimal: /float: ['0'-'9']+ '.' ['0'-'9']+/ #value;"
			
			"%start: decimal decimal;",
			
			"f1 = decimal[0].value;",
			"f2 = decimal[1].value;",
			
			"test01 = (3.5  + 2.5); %error:  6.0  == (3.5  + 2.5);",
			"test02 = (3.5  - 2.5); %error:  1.0  == (3.5  - 2.5);",
			"test03 = (3.5  * 2.5); %error:  8.75 == (3.5  * 2.5);",
			"test04 = (3.5  / 2.5); %error:  1.4  == (3.5  / 2.5);",
			"test05 = (3.5  % 2.5); %error:  1.0  == (3.5  % 2.5);",
			"test06 = (3.5 ** 2.0); %error: 12.25 == (3.5 ** 2.0);",
			
			"test07 = (3.5 >  2.5); %error: true  == (3.5 >  2.5);",
			"test08 = (3.5 >= 2.5); %error: true  == (3.5 >= 2.5);",
			"test09 = (3.5 == 2.5); %error: false == (3.5 == 2.5);",
			"test10 = (3.5 != 2.5); %error: true  == (3.5 != 2.5);",
			"test11 = (3.5 <= 2.5); %error: false == (3.5 <= 2.5);",
			"test12 = (3.5 <  2.5); %error: false == (3.5 <  2.5);",
			
			"%error: +3.0 == (+f1);",
			"%error: -3.0 == (-f1);",
			
			"%error: 5.5 == (f1  + 2.5);",
			"%error: 0.5 == (f1  - 2.5);",
			"%error: 7.5 == (f1  * 2.5);",
			"%error: 1.5 == (f1  / 2.0);",
			"%error: 1.0 == (f1  % 2.0);",
			"%error: 9.0 == (f1 ** 2.0);",
			
			"%error: true  == (f1 >  2.5);",
			"%error: true  == (f1 >= 2.5);",
			"%error: false == (f1 == 2.5);",
			"%error: true  == (f1 != 2.5);",
			"%error: false == (f1 <= 2.5);",
			"%error: false == (f1 <  2.5);",
		],
		
		"./test.txt": [
			"3.0 2.31"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp -lm",
		"./out ./test.txt"
	],
}]
















