
[{
	"files": {
		"./maia": "bin/test-build/yes-verbose/no-dotout/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"integer: /int: ['0'-'9']+/ #value;",
			
			"%parse: argv as integer integer;",
			
			"i1 = integer[0].value;",
			"i2 = integer[1].value;",
			
			"i3 = 3;",
			
			"i4 = 0x12;",
			
			"i5 = +5;",
			"i6 = -5;",
			"i7 = ~5;",
			
			"i1;",
			
			"%error: 1 == (1);",
			"%error: 2 == (2);",
			"%error: 3 == (3);",

			"%error: +1 == (+1);",
			"%error: -2 == (-2);",
			"%error: -6 == (~5);",

			"%error: 5 == (3  + 2);",
			"%error: 1 == (3  - 2);",
			"%error: 6 == (3  * 2);",
			"%error: 1 == (3  / 2);",
			"%error: 1 == (3  % 2);",
			"%error: 9 == (3 ** 2);",
			
			"%error: +7 == (+i1);",
			"%error: -7 == (-i1);",
			"%error: -8 == (~i1);",
			
			"%error:  9 == (i1  + 2); i1  + 2;",
			"%error:  5 == (i1  - 2); i1  - 2;",
			"%error: 14 == (i1  * 2); i1  * 2;",
			"%error:  3 == (i1  / 2); i1  / 2;",
			"%error:  1 == (i1  % 2); i1  % 2;",
			"%error: 49 == (i1 ** 2); i1 ** 2;",
		],
		
		"./test.txt": [
			"7 12"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]


















