
[{
	"files": {
		"./guard": "bin/test-build/yes-verbose/no-dotout/guard",
		
		"./input.guard": [
			"%skip: ' ' | '\\n';",
			
			"word: /['a'-'z']+/ #value;",
			
			"%parse: \"test1.txt\" as word;",
			"%parse: \"test2.txt\" as word;",
			
			"%error: len!(word) == 2;",
			
			"%warning: word[0].value == word[1].value;",
		],
		
		"./test1.txt": [
			"abc"
		],
		
		"./test2.txt": [
			"abc"
		]
	},
	"commands": [
		"./guard -i ./input.guard -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out"
	]
