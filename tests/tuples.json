[{
	"files": {
		"./maia": "bin/test-build/yes-verbose/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"integer: /int: ['0'-'9']+/ #value;",
			
			"%start: integer;",
			
			"u = integer[0].value;",
			
			"%error: () == (());",
			"%error: (1, 2, 3) == ((1, 2, 3));",
			"%error: (1, 1.5, true) == ((1, 1.5, true));",
			"%error: (1, ) == ((1, ));",
			
			"%error: (1, 2, 3, 4) == ((1, 2) + (3, 4));",
			
			"%error: false == ((1, 2) >  (3, 4));",
			"%error: false == ((1, 2) >= (3, 4));",
			"%error: true == ((    ) == (    ));",
			"%error: false == ((1, 2) == (3, 4));",
			"%error: true == ((1, 2) != (3, 4));",
			"%error: true == ((1, 2) <= (3, 4));",
			"%error: true == ((1, 2) <  (3, 4));",
			
			"%error: false == ((1, u) >  (3, 4));",
			"%error: false == ((1, u) >= (3, 4));",
			"%error: false == ((1, u) == (3, 4));",
			"%error: true == ((1, u) != (3, 4));",
			"%error: true == ((1, u) <= (3, 4));",
			"%error: true == ((1, u) <  (3, 4));",
			
			"%error: 2 == ((1, 2, 3).1);",
			"%error: u == ((1, u, 3).1);",
			"%error: 2 == ((1, 2, u).1);",
		],
		
		"./input.txt": [
			"7"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./input.txt"
	],
}]














