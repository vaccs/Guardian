[{
	"files": {
		"./maia": "bin/test-build/maia",
		
		"./input.maia": [
			"%skip: ' ';",
			"numbers: /int: ['0'-'9']+/ #value;",
			"%start: numbers numbers;",
			"f = $int x: $: $int y: $: x + y;",
			"x = numbers[0];",
			"y = numbers[1];",
			"%error: f(x)()(y)() == 7;"
		],
		
		"./test.txt": [
			"3 4"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out",
		"./out"
	]
}]
