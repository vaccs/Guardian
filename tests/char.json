
[{
	"files": {
		"./maia": "bin/test-build/yes-verbose/no-dotout/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"character: /char: ['a'-'z']/ #value;",
			
			"%parse: argv as character character;",
			
			"%print: 'a';",
			"%print: '\\\\';",
			"%print: '\\t';",
			"%print: '\\n';",
			
			"%error:  true == ('a' <  'b');",
			"%error:  true == ('a' <= 'b');",
			"%error: false == ('a' == 'b');",
			"%error:  true == ('a' != 'b');",
			"%error: false == ('a' >= 'b');",
			"%error: false == ('a' >  'b');",
			
			"a = character[0].value;",
			"b = character[1].value;",
			"c = 'c';",
			
			"%error:  true == (a <  b);",
			"%error:  true == (a <= b);",
			"%error: false == (a == b);",
			"%error:  true == (a != b);",
			"%error: false == (a >= b);",
			"%error: false == (a >  b);",
		],
		
		"./test.txt": [
			"a b"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
