
[{
	"files": {
		"./maia": "bin/release-build/yes-verbose/no-dotout/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"character: /char: ['a'-'z']/ #value;",
			
			"%start: character character;",
			
			"test01 = true == ('a' <  'b');",
			"test02 = true == ('a' <= 'b');",
			"test03 = false == ('a' == 'b');",
			"test04 = true == ('a' != 'b');",
			"test05 = false == ('a' >= 'b');",
			"test06 = false == ('a' >  'b');",
			
			"%error: test01 && test02 && test03 && test04 && test05 && test06;",
			
			"a = character[0].value;",
			"b = character[1].value;",
			"c = 'c';",
			
			"test07 = true == (a <  b);",
			"test08 = true == (a <= b);",
			"test09 = false == (a == b);",
			"test10 = true == (a != b);",
			"test11 = false == (a >= b);",
			"test12 = false == (a >  b);",
			
			"%error: test07 && test08 && test09 && test10 && test11 && test12;",
		],
		
		"./test.txt": [
			"a b"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]



