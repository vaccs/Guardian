
[{
	"files": {
		"./maia": "bin/test-build/yes-verbose/no-dotout/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"integer: /int: ['0'-'9']+/ #value;",
			
			"%parse: argv as integer;",
			
			"u = integer[0].value;",
			
			"t = [1, 2];",
			
			"%error: [<int>] == ([<int>]);",
			"%error: [1, 2, 3, 4] == ([1, 2, 3, 4]);",
			
			"%error: [1, 2, 3, 4] == ([1, 2] + [3, 4]);",
			"%error: [u, 2, 3, 4] == ([u, 2] + [3, 4]);",
			
			"%error: false == ([1, 2] >  [3, 4]);",
			"%error: false == ([1, 2] >= [3, 4]);",
			"%error: true  == ([<char>] == [<char>]);",
			"%error: false == ([1, 2] == [3, 4]);",
			"%error: true  == ([1, 2] != [3, 4]);",
			"%error: true  == ([1, 2] <= [3, 4]);",
			"%error: true  == ([1, 2] <  [3, 4]);",
			
			"%error: 2 == ([1, 2, 3][1]);",
			"%error: u == ([1, u, 3][1]);",
			"%error: 2 == ([1, 2, u][1]);",
			"%error: 8 == ([1, 2, 3, 4, 5, 6, 7, 8][u]);",
			
			"%error: true  == (\"a\" <  \"b\");",
			"%error: true  == (\"a\" <= \"b\");",
			"%error: true  == (\"\" == \"\");",
			"%error: false == (\"a\" == \"b\");",
			"%error: true  == (\"a\" != \"b\");",
			"%error: false == (\"a\" >= \"b\");",
			"%error: false == (\"a\" >  \"b\");",
			
			"%error: false == (\"abc\"[1] == \"c\");",
			"%error: true  == (\"abc\"[1] == \"b\");",
			
			"_ = 2 in [1, 2, 3];",
			
			"%error: true  == (2 in [1, 2, 3]);",
			"%error: false == (2 in [1, 5, 3]);",
			"%error: false == (2 in [1, u, 3]);",
			"%error: false == (u in [1, 2, 3]);",
			
			"%error: true  == (\"a\" in \"abc\");",
			"%error: false == (\"d\" in \"abc\");",
			
			"f = $int a, b -> int[]: [a, b];",
			
			"%error: f(3, 4) == [3] + [4];",
			"%error: f(u, 4) == [u] + [4];",
		],
		
		"./test.txt": [
			"7"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]
















