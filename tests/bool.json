
[{
	"files": {
		"./maia": "bin/release-build/yes-verbose/no-dotout/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"boolean: /bool: \"true\" | \"false\"/ #value;",
			
			"%start: boolean boolean;",
			
			"b1 = boolean[0].value;",
			"b2 = boolean[1].value;",
			"b3 = true;",
			"b4 = false;",
			
			"%error: true  == ( true);",
			"%error: false == ( false);",
			
			"%error: false == (!true);",
			"%error: true  == (!false);",
			
			"%error: true  == (true  || true );",
			"%error: true  == (true  || false);",
			"%error: true  == (false || true );",
			"%error: false == (false || false);",
			
			"%error: true  == (b1 || true );",
			"%error: true  == (b1 || false);",
			"%error: true  == (true  || b2);",
			"%error: false == (false || b2);",
			
			"%error: true  == (b1 || b2);",
			
			"%error: true  == (true  && true );",
			"%error: false == (true  && false);",
			"%error: false == (false && true );",
			"%error: false == (false && false);",
			
			"%error: true  == (b1    && true );",
			"%error: false == (b1    && false);",
			"%error: false == (true  && b2   );",
			"%error: false == (false && b2   );",
			"%error: false == (b1    && b2   );",
			
			"%error: false == (true  >  true );",
			"%error: true  == (true  >  false);",
			"%error: false == (false >  true );",
			"%error: false == (false >  false);",
			
			"%error: true  == (true  >= true );",
			"%error: true  == (true  >= false);",
			"%error: false == (false >= true );",
			"%error: true  == (false >= false);",
			
			"%error: true  == (true  == true );",
			"%error: false == (true  == false);",
			"%error: false == (false == true );",
			"%error: true  == (false == false);",
			
			"%error: false == (true  != true );",
			"%error: true  == (true  != false);",
			"%error: true  == (false != true );",
			"%error: false == (false != false);",
			
			"%error: true  == (true  <= true );",
			"%error: false == (true  <= false);",
			"%error: true  == (false <= true );",
			"%error: true  == (false <= false);",
			
			"%error: false == (true  <  true );",
			"%error: false == (true  <  false);",
			"%error: true  == (false <  true );",
			"%error: false == (false <  false);",

			"%error: true  == (true  implies true );",
			"%error: false == (true  implies false);",
			"%error: true  == (false implies true );",
			"%error: true  == (false implies false);",

			"%error: true  == (b1    implies true );",
			"%error: false == (b1    implies false);",
			"%error: false == (true  implies b2   );",
			"%error: true  == (false implies b2   );",
			"%error: false == (b1    implies b2   );",

			"%error: true  == (b1 >  b2);",
			"%error: true  == (b1 >= b2);",
			"%error: false == (b1 == b2);",
			"%error: true  == (b1 != b2);",
			"%error: false == (b1 <= b2);",
			"%error: false == (b1 <  b2);",
		],
		
		"./test.txt": [
			"true false"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]















