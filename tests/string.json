
[{
	"files": {
		"./guard": "bin/test-build/yes-verbose/no-dotout/guard",
		
		"./input.guard": [
			"%skip: ' ' | '\\n';",
			
			"string: /['a'-'z']+/ #value;",
			
			"%parse: argv as string{2};",
			
			"s1 = \"abc\";",
			"s2 = \"def\";",
			"s3 = string[0].value;",
			"s4 = string[1].value;",
			
			"%error: \"abcdef\" == (s1 + s2);",
			
			"%error: \"c\" == (s1[2]);",
			
			"%error: \"bc\" == (s1[1:]);",
			
			"%error: \"ab\" == (s1[:2]);",
			
			"%error: \"abcdef\" == (s1 + s2);",
			
			"%error: \"ab\" == ($string x, y -> string: x + y)(\"a\", \"b\");",
			
			"%error: true == (\"a\" in s1);",
			
			"%error: true == ($string x -> bool: x in s1)(\"a\");",
			
			"%error: \"c\" == ($int x -> string: s1[x])(2);",
			
			"%error: \"b\" == ($int s, e -> string: s1[s:e])(1, 2);",
			
			"%error: \"abcdef\" == (s3 + s4);",
			
			"%error: true == (\"a\" in s3);",
			
			"%error: \"c\" == (s3[2]);",
			
			"%error: \"bc\" == (s3[1:]);",
			
			"%error: false == (s1 >  s2);",
			"%error: false == (s1 >= s2);",
			"%error: false == (s1 == s2);",
			"%error: true  == (s1 != s2);",
			"%error: true  == (s1 <= s2);",
			"%error: true  == (s1 <  s2);",
			
			"%error: false == ($string a, b -> bool: a >  b)(s1, s2);",
			"%error: false == ($string a, b -> bool: a >= b)(s1, s2);",
			"%error: false == ($string a, b -> bool: a == b)(s1, s2);",
			"%error: true  == ($string a, b -> bool: a != b)(s1, s2);",
			"%error: true  == ($string a, b -> bool: a <= b)(s1, s2);",
			"%error: true  == ($string a, b -> bool: a <  b)(s1, s2);",
			
			"%error: false == (s3 >  s4);",
			"%error: false == (s3 >= s4);",
			"%error: false == (s3 == s4);",
			"%error: true  == (s3 != s4);",
			"%error: true  == (s3 <= s4);",
			"%error: true  == (s3 <  s4);",
		],
		
		"./test.txt": [
			"abc def"
		]
	},
	"commands": [
		"./guard -i ./input.guard -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]















