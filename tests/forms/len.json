
[{
	"files": {
		"./maia": "bin/release-build/yes-verbose/no-dotout/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"boolean: /bool: \"true\" | \"false\"/ #value;",
			
			"%start: boolean boolean;",
			
			"b = boolean[0].value;",
			
			"%error: 3 == len!([1, 2, 3]);",
			
			"%error: 2 == len!([b, b]);",
			
			"%error: 2 == len!({0: 0, 1: 1});",
			
			"%error: 2 == len!({0: b, 1: b});",
			
			"%error: 2 == len!((0, 1));",
			
			"%error: 2 == len!((b, b));",
			
			"%error: 1 == len!({b, b});",
		],
		
		"./test.txt": [
			"true false"
		]
	},
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]















