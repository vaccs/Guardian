[{
	"files": {
		"./maia": "bin/test-build/yes-verbose/no-dotout/maia",
		
		"./input.maia": [
			"%skip: ' ' | '\\n';",
			
			"integer: /int: ['0'-'9']+/ #value;",
			
			"decimal: /['0'-'9']+ '.' ['0'-'9']+/ #value;",
			
			"%parse: argv as integer decimal;",
			
			"t = float!(3);",
			
			"s = float!(\"3.5\");",
			
			"%error: ($int x -> float: float!(x))(3) == 3.0;",
			
			"%error: t == 3.0;",
			
			"%error: s == 3.5;",
			
			"a = integer[0].value;",
			
			"%error: float!(a + 2) == 4.0;",
			
			"%error: float!(decimal[0].value) == 4.0;",
		],
		
		"./test.txt": [
			"2 4.0"
		]
	},
	
	"commands": [
		"./maia -i ./input.maia -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]
