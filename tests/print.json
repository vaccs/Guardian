[{
	"files": {
		"./guard": "bin/test-build/yes-verbose/no-dotout/guard",
		
		"./input.guard": [
			"%skip: \"\\n\";",
			
			"%parse: argv as \"abc\";",
			
			"%print: 1;",
			
			"%print: 3.14;",
			
			"%print: \"a\";",
			"%print: \"b\";",
			"%print: \"c\";",
			"%print: \"\\t\";",
			"%print: \"\\n\";",
			"%print: \"\\\\\";",
			
			"%print: \"\";",
			"%print: \"abc\";",
			
			"%print: ();",
			"%print: (1, );",
			"%print: (1, 2, 3);",
			
			"%print: [1, 2, 3];",
			"%print: [<int>];",
			"%print: [<char>];",
			"%print: [<float>];",
			"%print: [<int[]>];",
			"%print: [<int{}>];",
			"%print: [<(bool, char)>];",
			"%print: [<bool -> char>];",
			"%print: [<$bool -> char>];",
			
			"%print: {<int>};",
			"%print: {1, 2, 3};",
			
			"%print: {<int: int>};",
			"%print: {1: 2, 2: 3, 3: 4};",
			
			"%print: $int x, y -> int: x + y;",
		],
		
		"./test.txt": [
			"abc"
		]
	},
	
	"commands": [
		"./guard -i ./input.guard -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]










