[{
	"files": {
		"./guard": "bin/test-build/yes-verbose/no-dotout/guardian",
		
		"./input.guard": [
			"%skip: ' ' | '\\n';",
			
			"%parse: argv as /['a', 'b', 'c']+/;",
		],
		
		"./test.txt": [
			"abc"
		]
	},
	"commands": [
		"./guard -i ./input.guard -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]
