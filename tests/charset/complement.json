[{
	"files": {
		"./guard": "bin/test-build/yes-verbose/no-dotout/guard",
		
		"./input.guard": [
			"%skip: ' ' | '\\n';",
			
			"%parse: argv as /[!('a', 'e', 'i', 'o', 'u')]+/+;",
		],
		
		"./test.txt": [
			"bcdfghjklmnpqrstvwxyz"
			"ABCDEFGHIJKLMNOPQURSTUVWXYZ"
		]
	},
	"commands": [
		"./guard -i ./input.guard -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}]
