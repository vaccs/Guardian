[{
	"files": {
		"./guard": "bin/test-build/yes-verbose/no-dotout/guard",
		
		"./input.guard": [
			"%skip: ' ' | '\\n';",
			
			"word: /['a'-'z']+/ #value;",
			
			"%parse: argv as word;",
			
			"%error: \"abc\" =~ /['a'-'c']+/;",
			
			"%error: ($string x -> bool: x =~ /['a'-'c']+/)(\"abc\");",
			
			"%warning: word[0].value =~ /['a'-'c']+/;",
		],
		
		"./test.txt": [
			"acababcbccbac"
		]
	},
	"commands": [
		"./guard -i ./input.guard -o out.c",
		"gcc -Wall -Werror ./out.c -o ./out -lgmp",
		"./out ./test.txt"
	],
}, {
	"files": {
		"./guard": "bin/test-build/yes-verbose/no-dotout/guard",
		
